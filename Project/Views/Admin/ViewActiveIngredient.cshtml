@model Project.Models.ActiveIngredient
@{
    ViewData["Title"] = "ViewActiveIngredient";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<section class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1>Active Ingredient Details</h1>
            </div>
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a asp-controller="Admin" asp-action="Dashboard"> Home</a></li>
                    <li class="breadcrumb-item active">Active Ingredient's Details</li>
                </ol>
            </div>
        </div>
    </div><!-- /.container-fluid -->
</section>
<!--</section>-->
<!-- Main content -->
<section class="content" style=" padding-left: 16px; padding-right: 16px;">
    <div class="row">
        <div class="col-md-6">
            <div class="card card-primary">
            
                <div class="card-body">
                    @*<div class="form-group">
                        <label asp-for="ID" class="control-label"></label><br />
                        @Model.ID
                    </div>*@
                    <div class="form-group">
                        <label asp-for="ActiveIngredientName">Active Ingredient Name:</label><br />
                        @Model.ActiveIngredientName
                    </div>



                </div>
                <!-- /.card-body -->
            </div>
            <!-- /.card -->
        </div>
    </div>
    <div class="row">
        <div class="col-12">
            <a class="btn btn-info btn-sm" asp-controller="admin" asp-action="getallactiveingredient">
                <i class="fas btn-default">
                </i>
                Back
            </a>
        </div>
    </div>
</section>
