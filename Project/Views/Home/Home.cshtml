<!-- CTA carousel -->
<section class="pb-5 mb-2 mb-md-4 mb-lg-5" style="padding-top: 80px;">
    <div class="bg-secondary pb-lg-4 pb-xl-5">
        <div class="container pb-4 pb-md-5">
            <div class="row">
                <div class="col-xl-5 col-lg-6" style="margin-top: -80px;">

                    <!-- Swiper tabs -->
                    <div class="swiper-tabs mx-auto" style="max-width: 526px;">
                        <div id="image-1" class="swiper-tab active">
                            <img src="~/Landing/assets/img/Telehealth.jpg" class="rounded-3" alt="Image">
                        </div>
                        <div id="image-2" class="swiper-tab">
                            <img src="~/Landing/assets/img/ePrescription.jpg" class="rounded-3" alt="Image">
                        </div>
                        <div id="image-3" class="swiper-tab">
                            <img src="~/Landing/assets/img/medical Record.jpg" class="rounded-3" alt="Image">
                        </div>
                    </div>
                </div>
                <div class="col-xl-5 col-lg-6 offset-xl-1 pt-2 pt-md-4 pt-lg-5 mt-4">

                    <!-- Slider prev/next buttons -->
                    <div class="d-flex justify-content-center justify-content-lg-start pb-3 mb-3">
                        <button type="button" id="prev" class="btn btn-prev btn-icon btn-sm me-2">
                            <i class="bx bx-chevron-left"></i>
                        </button>
                        <button type="button" id="next" class="btn btn-next btn-icon btn-sm ms-2">
                            <i class="bx bx-chevron-right"></i>
                        </button>
                    </div>

                    <!-- Swiper slider -->
                    <div class="swiper mx-0 mb-md-n2 mb-xxl-n3" data-swiper-options='{
                  "spaceBetween": 30,
                  "loop": true,
                  "tabs": true,
                  "navigation": {
                    "prevEl": "#prev",
                    "nextEl": "#next"
                  }
                }'>
                        <div class="swiper-wrapper text-center text-lg-start">

                            <!-- Item -->
                            <div class="swiper-slide pb-4" data-swiper-tab="#image-1">
                                <h2 class="h1 mb-4">Telehealth-Start Your Care <span class="text-success">Online</span> Now</h2>
                                <p class="pb-2 pb-xl-0 mb-4 mb-xl-5">Telehealth enables you to have a doctor's consultation from your home via video call or phone</p>
                                <a href="" class="btn btn-success btn-lg shadow-success">Learn more</a>
                            </div>

                            <!-- Item -->
                            <div class="swiper-slide pb-4" data-swiper-tab="#image-2">
                                <h2 class="h1 mb-4">Electronic <span class="text-success">Prescriptions</span> </h2>
                                <p class="pb-2 pb-xl-0 mb-4 mb-xl-5">Electronic prescriptions are now available to everyone. A convenient alternative to paper prescriptions, sent by your healthcare provider as an email or SMS.</p>
                                <a href="#" class="btn btn-success btn-lg shadow-success">Learn more</a>
                            </div>

                            <!-- Item -->
                            <div class="swiper-slide pb-4" data-swiper-tab="#image-2">
                                <h2 class="h1 mb-4">Electronic <span class="text-success">My Health Record</span> </h2>
                                <p class="pb-2 pb-xl-0 mb-4 mb-xl-5">My Health Record provides you secure access to your key healthcare information whenever you need it</p>
                                <a href="#" class="btn btn-success btn-lg shadow-success">Learn more</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>


